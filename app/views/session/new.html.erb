<h1>ログイン</h1>
<%= form_with url:signin_path, local:true do |f| %>
  <div class="from-group">
    <label>メールアドレス</label>
    <%= f.email_field :email, class: "form-control" %>
  </div>
  <div class="from-group">
    <label>パスワード</label>
    <%= f.password_field :password, class: "form-control" %>
  </div>
  <%= f.submit "ログイン", class: "btn btn-primary" %>
<% end %>